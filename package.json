{
  "name": "OpenRAP",
  "version": "1.0.0",
  "description": "OpenRAP Software",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf -rf dist && tsc && cpy env.json ./dist && cpy package.json ./dist && cpy  ./db/ ./dist/db/",
    "test": "nyc --reporter=html --reporter=text mocha --exit -r ts-node/register **/**/*.spec.ts **/*.spec.ts *.spec.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "**/*.spec.ts",
      "**/**/*.spec.ts",
      "*.spec.ts",
      "interfaces/*.ts"
    ],
    "include": [
      "**/**/*.ts",
      "**/*.ts",
      "*.ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  },
  "author": "sunbird",
  "license": "MIT",
  "dependencies": {
    "archiver": "^3.0.0",
    "body-parser": "^1.18.2",
    "chokidar": "^3.0.0",
    "decompress-zip": "^0.2.2",
    "express": "^4.16.3",
    "fs-extra": "^7.0.1",
    "hashids": "^1.2.2",
    "lodash": "^4.17.11",
    "nano": "^8.1.0",
    "su-downloader3": "^1.0.8",
    "typescript-ioc": "^1.2.5"
  },
  "devDependencies": {
    "@project-sunbird/ext-framework-server": "^0.1.21",
    "@types/archiver": "^2.1.3",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.9",
    "@types/unzipper": "^0.9.1",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "cpy-cli": "^2.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^2.8.4",
    "uuid": "^3.3.2"
  }
}